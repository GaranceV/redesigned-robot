[help]
    autocorrect = 15
[init]
    templatedir = ~/.git-templates
[alias]
    st = status
    ch = checkout
    pl = pull
    ph = push
    a = add
    co = commit -m
    amend = commit --amend --no-edit
    conflict = !git ls-files -u | cut -f 2 | sort -u
[log]
    abbrevCommit = true
[status]
    short = true
    branch = true
